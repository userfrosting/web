<div class="feeds">

{% for name, feed in twig_feeds %}
    <h3>{{ name }}</h3>
    {% for item in feed.items %}
    <div class="feed-item">
        <h5>
           <a href="{{ item.link }}" target="_blank">{{ item.title }}</a>
       </h5>
        <span>{{ item.lastModified|date("m/d/y") }}</span>
        {{ item.description|raw }}
    </div>
    {% endfor %}
{% endfor %}

